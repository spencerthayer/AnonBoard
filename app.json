{
  "name": "AnonBoard",
  "author": "Spencer Thayer",
  "version": "0.5.1",
  "description": "An anonymous flat file image/text board designed to make crypto and security culture easy.",
  "repository": "https://github.com/spencerthayer/AnonBoard.git",
  "logo": "https://raw.githubusercontent.com/spencerthayer/AnonBoard/master/favicon.png",
  "keywords": ["cryptojs", "chat", "secure", "encryption", "e2ee"],
  "env": {
    "SITE_NAME": {
      "description": "Change the site name to anythings.",
      "required": true
    },
    "MAX_POSTS": "0",
    "PROXY_URL": "https://ssl-proxy.my-addr.org/myaddrproxy.php/https/",
    "ADMIN_PASS": {
      "description": "Change to a strong password!",
      "required": true
    }
  }
}
